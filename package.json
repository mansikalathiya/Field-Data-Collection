{
  "name": "Observe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean:cache": "rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "clean:modules": "rm -rf node_modules && yarn",
    "clean": "npm run clean:cache && npm run clean:modules",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios && react-native log-ios",
    "android": "react-native run-android && react-native log-android",
    "android:install": "react-native run-android && adb install android/app/build/outputs/apk/app-debug.apk && react-native log-android",
    "precommit": "lint-staged",
    "test": "tape test/*.js"
  },
  "dependencies": {
    "@mapbox/tilebelt": "^1.0.1",
